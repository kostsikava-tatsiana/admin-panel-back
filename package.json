{
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  },
  "scripts": {
    "init-db": "node scripts/init-db.js",
